# API Keys for Vision Language Models
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_gemini_api_key_here

# Model Configuration
VLM_PROVIDER=openai  # Options: openai, google
VLM_MODEL=gpt-4o  # Options: gpt-4o, gpt-4-turbo, gemini-1.5-pro-vision

# Crawler Configuration
MAX_NAVIGATION_STEPS=20
SCREENSHOT_QUALITY=high  # Options: low, medium, high
TIMEOUT_SECONDS=30

# Dashboard Configuration
STREAMLIT_PORT=8501
ENABLE_LIVE_FEED=true
